{
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "quotes": 0,
    "no-trailing-spaces": 0,
    "eol-last": 0,
    "no-unused-vars": 0,
    "no-underscore-dangle": 0,
    "no-alert": 0,
    "no-lone-blocks": 0,
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "no-eval": 2,
    "no-empty-function": 1
  },
  "globals": {
    "jQuery": true,
    "$": true
  }
}
